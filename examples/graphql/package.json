{
  "name": "graphql",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "(cd ../../ && npx yarn@1.22.22 pack ../../ --filename ./examples/graphql/rxdb-local.tgz)",
    "refresh": "rimraf -r node_modules/rxdb/ && npm i --force",
    "server": "node ./server/index.js",
    "build": "concurrently \"webpack -c ./worker.webpack.config.js\" \"webpack -c ./webpack.config.js\"",
    "build:serve": "ws -p 8888 -d dist/"
  },
  "browser": {
    "fs": false
  },
  "private": true,
  "author": "pubkey",
  "license": "MIT",
  "type": "module",
  "//": "NOTICE: For the Github CI we use the local RxDB build (rxdb-local.tgz). In your app should just install 'rxdb' from npm instead",
  "dependencies": {
    "@apollo/server": "4.12.2",
    "concurrently": "9.2.0",
    "cors": "2.8.5",
    "express-graphql": "0.12.0",
    "graphql-client": "2.0.1",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.16.2",
    "local-web-server": "5.4.0",
    "normalize.css": "8.0.1",
    "pouchdb-adapter-idb": "9.0.0",
    "rxdb": "file:rxdb-local.tgz",
    "rxdb-premium": "16.17.2",
    "rxjs": "7.8.2",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "async-test-util": "2.5.0",
    "babel-loader": "^10.0.0",
    "css-loader": "7.1.2",
    "eslint": "8.57.1",
    "esm": "3.2.25",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.4",
    "mini-css-extract-plugin": "2.9.4",
    "ncp": "2.0.0",
    "rimraf": "5.0.10",
    "source-map-loader": "5.0.0",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "testcafe": "3.7.2",
    "testcafe-hammerhead": "31.7.5",
    "webpack": "5.101.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "accessTokens": {
    "rxdb-premium": "your_premium_token"
  }
}
